{
  "name": "pf-web-app",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Propertyfinder/pf-web-app"
  },
  "homepage": "https://github.com/Propertyfinder/pf-web-app",
  "scripts": {
    "dev": "DEV=1 yarn download-i18n && yarn download-resources && next dev -p 8080",
    "dev-desktop": "NEXT_PUBLIC_MOBILE= yarn dev",
    "dev-qa": "NEXT_PUBLIC_COUNTRY_CODE=qa yarn dev",
    "dev-qa-desktop": "NEXT_PUBLIC_COUNTRY_CODE=qa yarn dev-desktop",
    "dev-sa": "NEXT_PUBLIC_COUNTRY_CODE=sa yarn dev",
    "dev-sa-desktop": "NEXT_PUBLIC_COUNTRY_CODE=sa yarn dev-desktop",
    "dev-bh": "NEXT_PUBLIC_COUNTRY_CODE=bh yarn dev",
    "dev-bh-desktop": "NEXT_PUBLIC_COUNTRY_CODE=bh yarn dev-desktop",
    "dev-ma": "NEXT_PUBLIC_COUNTRY_CODE=ma yarn dev",
    "dev-ma-desktop": "NEXT_PUBLIC_COUNTRY_CODE=ma yarn dev-desktop",
    "dev-lb": "NEXT_PUBLIC_COUNTRY_CODE=lb yarn dev",
    "dev-lb-desktop": "NEXT_PUBLIC_COUNTRY_CODE=lb yarn dev-desktop",
    "dev-eg": "NEXT_PUBLIC_COUNTRY_CODE=eg yarn dev",
    "dev-eg-desktop": "NEXT_PUBLIC_COUNTRY_CODE=eg yarn dev-desktop",
    "dev-dbg": "NODE_OPTIONS='--inspect' yarn dev",
    "dev-desktop-dbg": "NODE_OPTIONS='--inspect' yarn dev-desktop",
    "build": "yarn download-i18n && yarn download-resources && ./scripts/build.js",
    "start": "next start -p 8080",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./story-book",
    "prettier-check": "prettier --check",
    "prettier-fix": "prettier --write",
    "prepare": "husky install",
    "setup-vscode": "node ./scripts/setup-vscode",
    "test": "jest",
    "test:watch": "jest -â€”watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage=true --collectCoverageFrom=$1",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "download-resources": "./scripts/download-resources/run.sh",
    "randomize-obfuscation-keys": "./scripts/randomize-obfuscation-keys.js",
    "lint": "yarn eslint src --ext .ts,.tsx && yarn stylelint 'src/**/*.{css,scss}'",
    "validate-types": "tsc --project ./tsconfig.json",
    "download-i18n": "node ./scripts/download-translations",
    "build-eslint": "rm -rf ./utils/eslint/result && tsc --project ./utils/eslint/tsconfig.json && cp ./utils/eslint/package.json ./utils/eslint/result && yarn add --dev file:./utils/eslint/result"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,css,scss,md}": "prettier --check",
    "src/**/*.{css,scss}": "stylelint --quiet",
    "src/**/*.{js,ts,tsx}": "eslint --max-warnings=0",
    "src/**/*spec.{js,ts,tsx}": "yarn test:coverage"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "i18next": "20.3.2",
    "intersection-observer": "0.12.0",
    "next": "11.0.1",
    "next-http-proxy-middleware": "1.0.10",
    "next-i18next": "8.5.1",
    "next-plugin-preact": "3.0.6",
    "preact": "10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-i18next": "^11.11.4",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "sass": "^1.35.1",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@lokalise/node-api": "^6.3.0",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@storybook/theming": "^6.3.2",
    "@svgr/webpack": "^5.5.0",
    "@swc/jest": "^0.1.4",
    "@types/enzyme": "^3.10.9",
    "@types/eslint": "^7.2.14",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.1.0",
    "enzyme-to-json": "^3.6.2",
    "esbuild": "^0.12.17",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-preact": "^1.1.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-pf-rules": "file:./utils/eslint/result",
    "extract-zip": "^2.0.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-module-name-mapper": "^0.1.5",
    "lint-staged": "^11.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "sass-loader": "^10",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-declaration-strict-value": "^1.7.12",
    "stylelint-order": "^4.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "5.42.0"
  }
}
