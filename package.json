{
  "name": "pf-web-app",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Propertyfinder/pf-web-app"
  },
  "homepage": "https://github.com/Propertyfinder/pf-web-app",
  "scripts": {
    "dev": "next dev -p 8080",
    "dev-desktop": "NEXT_PUBLIC_MOBILE= next dev -p 8080",
    "dev-qa": "NEXT_PUBLIC_COUNTRY_CODE=qa next dev -p 8080",
    "dev-qa-desktop": "NEXT_PUBLIC_COUNTRY_CODE=qa NEXT_PUBLIC_MOBILE= next dev -p 8080",
    "dev-dbg": "NEXT_PUBLIC_MOBILE=1 NODE_OPTIONS='--inspect' next dev -p 8080",
    "dev-desktop-dbg": "NEXT_PUBLIC_MOBILE= NODE_OPTIONS='--inspect' next dev -p 8080",
    "build": "./scripts/build.sh",
    "start": "next start -p 8080",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./story-book",
    "prettier-check": "prettier --check",
    "prettier-fix": "prettier --write",
    "prepare": "husky install",
    "setup-vscode": "node ./scripts/setup-vscode"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss,md}": "prettier --check",
    "*.css": "stylelint --quiet",
    "src/**/*.{js,ts,tsx}": "eslint --max-warnings=0"
  },
  "dependencies": {
    "i18next": "20.3.2",
    "next": "11.0.1",
    "next-http-proxy-middleware": "1.0.10",
    "next-i18next": "8.5.1",
    "next-plugin-preact": "3.0.6",
    "preact": "10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "sass": "^1.35.1",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@storybook/theming": "^6.3.2",
    "@svgr/webpack": "^5.5.0",
    "@types/eslint": "^7.2.14",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-preact": "^1.1.4",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-declaration-strict-value": "^1.7.12",
    "stylelint-order": "^4.1.0",
    "css-loader": "^5.0.1",
    "style-loader": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "5.42.0"
  }
}
