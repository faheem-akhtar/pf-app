{
  "name": "pf-web-app",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Propertyfinder/pf-web-app"
  },
  "homepage": "https://github.com/Propertyfinder/pf-web-app",
  "scripts": {
    "dev": "NEXT_PUBLIC_MOBILE=1 next dev -p 8080",
    "dev-desktop": "NEXT_PUBLIC_MOBILE= next dev -p 8080",
    "dev-dbg": "NEXT_PUBLIC_MOBILE=1 NODE_OPTIONS='--inspect' next dev -p 8080",
    "dev-desktop-dbg": "NEXT_PUBLIC_MOBILE= NODE_OPTIONS='--inspect' next dev -p 8080",
    "build": "rm -rf .mobile.next && rm -rf .desktop.next && NEXT_PUBLIC_MOBILE=1 next build && NEXT_PUBLIC_MOBILE= next build",
    "start": "next start -p 8080",
    "start-desktop": "NEXT_PUBLIC_MOBILE=1 next start -p 8080"
  },
  "dependencies": {
    "i18next": "20.3.2",
    "next": "11.0.1",
    "next-http-proxy-middleware": "1.0.10",
    "next-i18next": "8.5.1",
    "next-plugin-preact": "3.0.6",
    "preact": "10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "5.42.0"
  }
}
